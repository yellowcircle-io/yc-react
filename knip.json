{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "src/main.jsx",
    "src/RouterApp.jsx"
  ],
  "project": [
    "src/**/*.{js,jsx}"
  ],
  "ignore": [
    ".claude/**",
    "dev-context/**",
    "functions/**",
    "scripts/**",
    "my-project/**",
    "yellowcircle-outreach/**",
    "docs/**",
    "src/archive/**",
    "*.config.js",
    "src/App.jsx",
    "src/utils/toast.js",
    "src/utils/notificationManager.js",
    "src/components/notifications/NotificationBell.jsx",
    "src/hooks/usePresence.js",
    "src/components/unity/ActiveUsersIndicator.jsx",
    "src/components/unity/UserAvatar.jsx"
  ],
  "ignoreDependencies": [
    "@builder.io/dev-tools",
    "@rollup/rollup-linux-x64-gnu",
    "autoprefixer",
    "postcss",
    "tailwindcss",
    "@tailwindcss/vite",
    "html2canvas",
    "playwright",
    "esbuild",
    "husky",
    "lint-staged"
  ],
  "ignoreExportsUsedInFile": true
}
